{% extends 'base.html.twig' %}

{% block title %}Feedback{% endblock %}

{% block content %}
    <h1>Feedback</h1>

    <table class="table">
        <tbody>

            <tr>
                <th>Nom</th>
                <td>{{ feedback.nom }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ feedback.email }}</td>
            </tr>
            <tr>
                <th>Message</th>
                <td>{{ feedback.message }}</td>
            </tr>
            <tr>
                <th>Etat</th>
                <td>{{ feedback.etat }}</td>
            </tr>

        </tbody>
    </table>

    <a href="{{ path('feedback_index') }}">back to list</a>

    <a href="{{ path('feedback_edit', {'id': feedback.id}) }}">edit</a>

    {{ include('feedback/_delete_form.html.twig') }}
{% endblock %}
