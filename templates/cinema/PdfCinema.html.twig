<style>
    body {
        margin: 0;
        font-family: "Roboto", "Helvetica", "Arial", sans-serifsans-serif;
        font-size: 1rem;
        font-weight: 400;
        line-height: 1.5;
        color: #212529;
        text-align: left;
        background-color: #fafafa;
    }
    table {
        font-family: arial, sans-serif;
        border-collapse: collapse;
        width: 100%;
    }

    td, th {
        border: 1px solid #dddddd;
        text-align: left;
        padding: 8px;
    }

    tr:nth-child(even) {
        background-color: #dddddd;
    }
    thead {
        display: table-header-group;
    }
</style>
<img src="data:image/png;base64,">
{% block content %}
    <h1>Nos Cinemas</h1>

    <table class="table">
        <thead>
        <tr>
            <th>
                Nom
            </th>
            <th>
                DateCreation
            </th>
            <th>
                Adresse
            </th>
            <th>
                Email
            </th>

        </tr>
        </thead>
        <tbody>
        {% for c in cinemas %}
            <tr>
                <td>{{ c.nom }}</td>
                <td>{{ c.dateCreation ? c.dateCreation|date('Y-m-d') : '' }}</td>
                <td>{{ c.adresse }}</td>
                <td>{{ c.email }}</td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
    </body>

{% endblock %}
